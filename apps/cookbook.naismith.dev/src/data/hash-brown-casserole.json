  "images": ["../images/recipes/hash-brown-casserole.jpg"],
  "stars": 4.5,
  "ingredients": [
    "2 lb frozen hash browns",
    "500 mL container sour cream",
    "1 can cream of chicken",
    "1 can cream of mushroom",
    "1/2 cup melted butter",
    "1 small grated onion",
    "2 cups grated cheddar cheese",
    "Parmesan cheese"
  ],
  "directions": [
    "Preheat oven at 350 degrees Fahrenheit",
    "Thaw potatoes slightly for easier mixing.",
    "In a 2x13 inch pan mix all ingredients except Parmesan cheese.",
    "Spread in pan, top with light coating of Parmesan.",
    "Bake 1 hour to 1 1/2 hours."
  ]
}
